{
  "name": "css-animation",
  "version": "2.0.3",
  "description": "css-animation",
  "keywords": [
    "css-animation"
  ],
  "homepage": "http://github.com/yiminghe/css-animation",
  "author": "",
  "repository": {
    "type": "git",
