{
  "name": "css-animation",
  "version": "1.6.0",
  "description": "css-animation",
  "keywords": [
    "css-animation"
  ],
  "homepage": "http://github.com/yiminghe/css-animation",
  "author": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:yiminghe/css-animation.git"
  },
  "bugs": {
    "url": "http://github.com/yiminghe/css-animation/issues"
  },
  "license": "MIT",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "scripts": {
    "prettier": "prettier --write \"{src,stories}/**/*.{js,tsx}\"",
    "start": "start-storybook -p 6006",
    "pub": "npm run build && npm publish pkg",
    "build": "pack build",
    "postbuild": "ln -s dist-src pkg/es && ln -s dist-src pkg/lib",
    "deploy": "storybook-to-ghpages",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@pika/pack": "^0.4.0",
    "@pika/plugin-build-types": "^0.5.1",
    "@pika/plugin-build-web": "^0.5.1",
    "@pika/plugin-standard-pkg": "^0.5.1",
    "@pika/types": "^0.5.1",
    "@storybook/react": "^5.1.9",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-loader": "^8.0.6",
    "lint-staged": "^9.2.1",
    "pre-commit": "1.x",
    "prettier": "^1.18.2",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "lint-staged": {
    "*.{tsx,js,jsx,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "dependencies": {
    "component-classes": "^1.2.6"
  }
}
